{
  "functions": [
    {
      "source": "firebase-functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ],
      "predeploy": ["npm --prefix ../app/server run build"]
    }
  ],
  "hosting": {
    "public": "firebase-functions/static",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/**",
        "function": "default"
      }
    ]
  }
}
